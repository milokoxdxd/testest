			end

			-- 5) Teleport Farm
			do
				local tpBind = nil
				createToggleRow(
					contentFrame,
					" Teleport Farm",
					true,
					function() return TPFarm_Enabled end,
					TPFarm_Enable,
					TPFarm_Disable,
					function() return tpBind end,
					function(k) tpBind = k end
				)
				createTPFarmTargetRow(contentFrame)
			end

