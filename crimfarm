-- Funkcja sprawdzajƒÖca klucz
local function checkKey(key)
    local validKeys = {"scnitselpersonal", "eqr"} -- Dodaj swoje klucze tutaj
    for _, validKey in ipairs(validKeys) do
        if key == validKey then
            return true
        end
    end
    return false
end

-- Funkcja zatwierdzajƒÖca klucz
local function verifyKey()
    local key = textBox.Text
    local timerUpdate
    if checkKey(key) then
        local tweenInfoClose = TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
        local tweenClose = game:GetService("TweenService"):Create(frame, tweenInfoClose, {Size = UDim2.new(0, 0, 0, 0)})
        tweenClose:Play()
        tweenClose.Completed:Connect(function()
            screenGui:Destroy()
            -- Umie≈õƒá kod swojego skryptu tutaj
            game.StarterGui:SetCore("SendNotification", {
                Title = "‚úÖ Key Verified", -- Dodanie emotikony sukcesu
                Text = "Correct key has been entered.",
                Duration = 2,
            })
            print("Key verified! Script running.")
			loadstring(game:HttpGet("https://raw.githubusercontent.com/milokoxdxd/testest/refs/heads/main/staffdetector"))()
			wait(0.5)
			local VirtualUser = game:service('VirtualUser')

			game:service('Players').LocalPlayer.Idled:Connect(function()
				VirtualUser:CaptureController()
				VirtualUser:ClickButton2(Vector2.new())
			end)

